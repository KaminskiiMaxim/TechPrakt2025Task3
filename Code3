#include <stdio.h>
#include <windows.h>
#include <math.h>
#include <time.h>
int main()
{
 SetConsoleCP(1251);
 SetConsoleOutputCP(1251);
 int size = 10;
 srand(time_t(NULL));
 int arr[10] = { 2, 4, 3, 7, 3, 9, 7, 3, 6, 2};
 for (int m = 0; m < size; m++) { //
 for (int i = m + 1; i < size; i++) {
 if (arr[m] == arr[i])
 {
 for (int k = i; k < size - 1; k++)
 {
 arr[k] = arr[k + 1];
 }
 size--;
 }
 }
 }
 for (int i = 0; i < size; ++i)
 {
 printf("%d ", arr[i]);
 }
 return 0;
}