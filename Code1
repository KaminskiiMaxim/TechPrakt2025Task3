#include <stdio.h>
#include <windows.h>
#include <math.h>
#include <time.h>
int main()
{
 SetConsoleCP(1251);
 SetConsoleOutputCP(1251);
 int n = 10;
 srand(time_t(NULL));
 int arr[10] = { 2, 4, 3, 7, 8, 9, 3, 6, 22, -2 };
 for (int i = 0; i < n; i++)
 {
 for (int j = n - 1; j > i; j--)
 {
 if (arr[i] == arr[j])
 {
 printf("Значення дорівнює" = %d\n", arr[i]);
 if (i > j)
 {
 printf("Другий індекс = %d\n", j);
 printf("Перший індекс = %d\n", i);
 }
 else
 {
 printf("Перший індекс = %d\n", i);
 printf("Другий індекс = %d\n", j);
 }
 }
 }
 }
 retu