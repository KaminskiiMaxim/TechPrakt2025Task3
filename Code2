#include <stdio.h>
#include <windows.h>
#include <math.h>
#include <time.h>
int main()
{
 SetConsoleCP(1251);
 SetConsoleOutputCP(1251);
 srand(time_t(NULL));
 int n = 10, sort;
 int arrA[5] = { 1,3,5,7,9 };
 int arrB[5] = { 2,4,6,8,10 };
 int arrC[10];
 for (int i = 0; i < 5; ++i)
 {
 arrC[i] = arrA[i];
 arrC[i + 5] = arrB[i];
 }
 for (int i = 0; i < 10; ++i) {
 for (int j = 1; j < 10 - i; ++j) {
 if (arrC[j-1] > arrC[j])
 {
 sort = arrC[j-1];
 arrC[j-1] = arrC[j];
 arrC[j] = sort;
 }
 }
 }
 for (int i = 0; i < 10; ++i)
 {
 printf("%d ", arrC[i]);
 }
 return 0;
}